{
  "name": "openbot-discord",
  "version": "1.0.0",
  "description": "An open source, fully customizable bot creating interface",
  "main": "server/build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node build/index.js",
    "watch:client": "cd client && npm run watch && cd ..",
    "watch:server": "npx nodemon index.ts",
    "build:server": "npx tsc",
    "build:client": "cd client && npm run build && cd ..",
    "build": "npm run build:client && npm run build:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinTechy/OpenBot-Discord.git"
  },
  "author": "MartinTechy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinTechy/OpenBot-Discord/issues"
  },
  "homepage": "https://github.com/MartinTechy/OpenBot-Discord#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.34",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": "15.11.0"
  }
}
